    {
      "parameters": {
        "jsCode": "// Tomamos el texto de entrada desde n8n\nconst inputText = $input.first().json.Texto || \"\";\n\n// Objeto para agrupar tags por estado final\nconst estadosAgrupados = {};\n\n// Mapa de traducción de estados a frases\nconst estadoAFraza = {\n    \"Sin empezar\": \"Se realiza el desmontaje de\",\n    \"Calibrado\": \"Se realiza la calibración de\",\n    \"🔴 Montado, no conexionado\": \"Se realiza el montaje de\",\n    \"🟡 Montado conexionado, no probado\": \"Se realiza el conexionado de\",\n    \"🟢 Precomisionado, ok\": \"Se realiza el precomisionado de\"\n};\n\n// Orden deseado\nconst ordenEstados = [\n    \"Sin empezar\",\n    \"Calibrado\",\n    \"🔴 Montado, no conexionado\",\n    \"🟡 Montado conexionado, no probado\",\n    \"🟢 Precomisionado, ok\"\n];\n\n// Separar cada línea\nconst lines = inputText.split('\\n').filter(l => l.trim() !== '');\n\n// Procesar cada línea\nfor (const line of lines) {\n    // Extraer el tag\n    const tagMatch = line.match(/TAG:\\s*(.*?)\\s+y/);\n    const tag = tagMatch ? tagMatch[1] : 'sin_tag';\n\n    // Extraer el estado final (después de 'a \"')\n    const estadoMatch = line.match(/a\\s*\"([^\"]+)\"/);\n    const estadoFinal = estadoMatch ? estadoMatch[1] : 'Sin Estado';\n\n    // Agrupar por estado final\n    if (!estadosAgrupados[estadoFinal]) {\n        estadosAgrupados[estadoFinal] = [];\n    }\n    estadosAgrupados[estadoFinal].push(tag);\n}\n\n// Construir el resultado final en el orden deseado\nconst resultados = [];\nfor (const estado of ordenEstados) {\n    if (estadosAgrupados[estado] && estadosAgrupados[estado].length > 0) {\n        const frase = estadoAFraza[estado];\n        resultados.push(`${frase} (${estadosAgrupados[estado].length} equipos): ${estadosAgrupados[estado].join(', ')}`);\n    }\n}\n\n// Concatenamos todo en un solo string con saltos de línea\nconst resultadoFinal = resultados.join('\\n');\n\n// Retornamos como un único item\nreturn [{ json: { message: resultadoFinal } }];\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -200,
        180
      ],
      "id": "625e628c-854d-41c5-87a5-8b4b87132ab6",
      "name": "Code"
    },
    {
      "parameters": {
        "jsCode": "// Loop over all input items\nfor (const item of $input.all()) {\n    if (item.json.x_studio_log_estadoanterior === \"Sin Instalar\") {\n        item.json.x_studio_log_estadoanterior = \"Sin Instalar\";\n    } else if (item.json.x_studio_log_estadoanterior === \"Instalado, sin probar\") {\n        item.json.x_studio_log_estadoanterior = \"🔴 Montado, no conexionado\";\n    } else if (item.json.x_studio_log_estadoanterior === \"Probado, falla\") {\n        item.json.x_studio_log_estadoanterior = \"🟡 Montado conexionado, no probado\";\n    } else if (item.json.x_studio_log_estadoanterior === \"Probado, listo\") {\n        item.json.x_studio_log_estadoanterior = \"🟢 Precomisionado, ok\";\n    }\n}\nfor (const item of $input.all()) {\n    if (item.json.x_studio_log_estadonuevo === \"Sin Instalar\") {\n        item.json.x_studio_log_estadonuevo = \"Sin Instalar\";\n    } else if (item.json.x_studio_log_estadonuevo === \"Instalado, sin probar\") {\n        item.json.x_studio_log_estadonuevo = \"🔴 Montado, no conexionado\";\n    } else if (item.json.x_studio_log_estadonuevo === \"Montado, no conexionado\") {\n        item.json.x_studio_log_estadonuevo = \"🔴 Montado, no conexionado\"; \n    }  else if (item.json.x_studio_log_estadonuevo === \"Probado, falla\") {\n        item.json.x_studio_log_estadonuevo = \"🟡 Montado conexionado, no probado\";\n    } else if (item.json.x_studio_log_estadonuevo === \"Probado, listo\") {\n        item.json.x_studio_log_estadonuevo = \"🟢 Precomisionado, ok\";\n    }\n}\n// Return the modified items\nreturn $input.all();\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -500,
        420
      ],
      "id": "4216738a-fe93-4dc2-83b0-d0d4936440bc",
      "name": "Code1"
    },
    {
      "parameters": {
        "jsCode": "// Iteramos sobre todos los items\nfor (const item of $input.all()) {\n    // Tomamos la fecha de Odoo\n    let createDateUTC = item.json.create_date; // ej: \"2025-09-22 11:10:25\"\n    \n    if (createDateUTC) {\n        // Convertimos a formato ISO\n        createDateUTC = createDateUTC.replace(' ', 'T');\n\n        // Convertimos a hora Argentina\n        const createDateArgentina = DateTime.fromISO(createDateUTC, { zone: 'utc' }) // Odoo guarda en UTC\n            .setZone('America/Argentina/Buenos_Aires') // convertir a Argentina\n            .toFormat(\"yyyy-MM-dd HH:mm:ss\");\n\n        // Agregamos la nueva propiedad al item\n        item.json.create_date_arg = createDateArgentina;\n    } else {\n        // Si no hay fecha, ponemos null\n        item.json.create_date_arg = null;\n    }\n}\n\n// Devolvemos todos los items con la nueva propiedad\nreturn $input.all();\n"
      },
      "type": "n8n-nodes-base.code",
